k=input()
b=0
a=0
l=0
o=0
n=0
for i in k:
    if i=='b':
        b+=1
    elif i=='a':
        a+=1
    elif i=='l':
        l+=1
    elif i=='o':
        o+=1
    elif i=='n':
        n+=1
    else:
        pass
c=0
while(1):
    b-=1
    a-=1
    l-=2
    o-=2
    n-=1
    if(b<0 or a<0 or l<0 or o<0 or n<0):
        break
    else:
        c+=1
print(c)
